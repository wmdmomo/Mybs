<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="#ffff" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        >
        <shape>
            <solid android:color="#000" />
        </shape>

    </item>
</layer-list>

